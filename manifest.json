{
    "manifest_version": 3,
    "name": "lmao",
    "description": "lmao",
    "version": "1.0",
    "permissions": [
        "scripting",
        "activeTab",
        "tabs",
        "storage",
        "background"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "host_permissions": [
        "https://www.facebook.com/watch/*",
        "https://www.youtube.com/*"
    ]
}
